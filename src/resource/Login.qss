#avatarView{
min-width: 100px;
min-height: 100px;
max-width:100px;
max-height: 100px;
border-radius: 50px;
background-color: rgb(75, 255, 225);
}
#connectBtn{
  color:#ffffff;
  background: #0275FF;
  border-radius: 3px;
}
#connectBtn:hover,#connectBtn:pressed{
  background: rgb(70,98,217);
}
QLineEdit:enabled{
  border: 1px solid #999999;
  border-radius: 3px;
  color:#666666;
  font-size:14pt;
}
QLineEdit:!enabled{
  border: 1px solid #CCCCCC;
  color:#B3B3B3;
}
